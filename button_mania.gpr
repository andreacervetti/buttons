with "gtkada";

project Button_Mania is

   for Create_Missing_Dirs use "True";
   for Languages use ("Ada");
   for Main use ("main.adb");
   for Source_Dirs use ("src/**");
   for Object_Dir use "obj/";
   for Exec_Dir use ".";

   package Builder is
      for Executable ("main.adb") use "buttons";
   end Builder;

end Button_Mania;

